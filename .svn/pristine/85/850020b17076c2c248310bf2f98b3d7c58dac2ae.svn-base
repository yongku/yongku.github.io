html,body{
    width:100%;
    height:100%;
}
body > div{
    width:100%;
    height:100%;
}
body > div > div{
    width:100%;
    height:100%;
}
/* title style */
.title{
    height:100%;
}
.title ul{
    width:100%;
    height:100%;
}
.title ul li{
    width:100%;
    height:100%;
}
.title img{
    height:100%;
    width:100%;
}
.sImage{
    height:100%;
    width:100%;
    position:absolute;
    z-index:0;
}
.sImage:nth-child(1){
    z-index:1;
}
.on{
    animation-name:moveimg;
    animation-duration:4s;
    z-index:1;
}
.aground{
    position:absolute;
    width:100%;
    height:100%;
    top:0;
    left:0;
    text-align:center;
    background:rgba(0,0,0,0.5);
    z-index:10;
}
.aground strong{
    color:#fff;
}
@keyframes moveimg{
    0% {opacity:0;}
    100% {opacity:1;}
}
@keyframes tText{
    0%{opacity:0;}
    50%{opacity:1;}
    100% {opacity:0;}
}
.aground strong{
    position:absolute;
    display:inline-block;
    animation-name:tText;
    animation-duration:4s;
    animation-iteration-count:infinite;
    top:50%;
    left:50%;
    font-size:200px;
    font-weight:800;
    transform:translate(-50%,-50%);
}
/* tnb style */
.tnbWrap{
    width:100%;
    height:60px;
    background:#26292e;
}
.tnb{
    position:relative;
    width:80%;
    height:100%;
    margin:0 auto;
    background:#26292e;
}
.fix{
    position:fixed;
    height:60px;
    top:0;
    z-index:100;
}
.local{
    color:#f00;
}
.tnb::after{
    content:"";
    display:block;
    clear:both;
}
.tnb h1{
    float:left;
    width:40%;
}
.tnb li{
    float:left;
    width:15%;
    text-align:center;
    line-height:60px;
    cursor:pointer;
}
@keyframes logo {
    6%  { color:#f00; }
    20% { color:#f80; }
    34% { color:#ff0; }
    48% { color:#0f0; }
    62% { color:#08f; }
    76% { color:#00f; }
    90% { color:#f0f; }
}
@keyframes blind{
    0%{}
    50%{color:#08f;}
    100%{}
}
.tnb h1 strong{
    padding-left:10px;
    font-size:40px;
}
.tnbMenu{
    position:relative;
}
.tnbMenu strong{
    font-size:25px;
    font-weight:800;
    color:#fff;
}
.tnbUnderbar{
    position:absolute;
    width:100%;
    height:2px;
    left:0;
    bottom:0;
}
/* portfolio style */
.portfolio{
    position:relative;
    background:#2f3238;
}
.poTitle{
    width:80%;
    height:20%;
    margin:0 auto;
    text-align:center;
}
.poTitle h2{
    font-size:60px;
    font-weight:600;
    line-height:100px;
    color:#fff;
}
.poTitle h2::first-letter{
    color:#f80;
}
.poTitle p{
    font-size:20px;
    font-weight:600;
    color:#fff;
}
.poInner{
    width:80%;
    height:68%;
    margin:0 auto;
}
.poMenu{
    float:left;
    width:13%;
    height:100%;
}
.poMenu ul{
    width:100%;
    height:100%;
    margin:20% 0;
}
.poMenu li{
    width:100%;
    height:15%;
    text-align:center;
    line-height:70px;
    cursor:pointer;
}
.poMenu li:first-child span{
    color:#ff0;
}
.poMenu span{
    font-size:20px;
    font-weight:600;
    color:#fff;
}
.poList{
    float:right;
    width:87%;
    height:100%;
    background:#2f3238;
    border:3px solid #ddd;
    border-radius:6px;
}
.poList ul{
    overflow-y:scroll;
    width:96%;
    height:100%;
    margin-left:4%;
}
.poList li{
    position:relative;
    float:left;
    width:200px;
    height:200px;
    margin:2% 1.5%;
    cursor:pointer;
}
.poList li:hover .cover{
    display:block;
}
.poList li div{
    width:100%;
    height:80%;
}
.poList img{
    width:100%;
    height:100%;
    border-radius:10%;
}
.cover{
    position:absolute;
    display:none;
    top:0;
    left:0;
    border-radius:10%;
    background:rgba(255, 0, 0,0.5)
}
.cover img{
    position:absolute;
    width:20%;
    height:20%;
    top:50%;
    left:50%;
    transform:translate(-50%,-50%);
}
.poList li .poMove{
    width:100%;
    height:20%;
    padding:10px;
    text-align:center;
}
.poList .poMove a{
    font-size:16px;
    color:#fff;
}
.poMove:hover a{
    color:#ff0;
}
/* profile style */
.profile{
    background:#26292e;
}
.proTitle{
    width:80%;
    height:20%;
    margin:0 auto;
    padding-top:5%;
    text-align:center;
}
.proTitle h2{
    font-size:60px;
    font-weight:600;
    line-height:80px;
    color:#888;
}
.proTitle h2::first-letter{
    color:#ff0;
}
.proTitle p{
    font-size:20px;
    font-weight:600;
    color:#888;
}
.proInner{
    width:80%;
    height:80%;
    margin:0 auto;
}
.proInner div{
    float:left
}
.proImg{
    width:40%;
    height:100%;
}
.proImg img{
    width:60%;
    height:80%;
    margin:10%;
}
.proText{
    width:60%;
    height:100%;
    text-align:center;
}
.proText ul{
    height:100%;
}
.proText > ul > li{
    height:10%;
}
.proText > ul > li strong{
    white-space: nowrap;
}
.proText strong{
    font-size:20px;
    line-height:100px;
    color:#888;
}
/* contact style */
.contact{
    background:#2f3238;
    padding-top:5%;
}
.conTitle{
    width:80%;
    height:20%;
    margin:0 auto;
    text-align:center;
}
.conTitle h2{
    font-size:60px;
    font-weight:600;
    line-height:100px;
    color:#fff;
}
.conTitle h2::first-letter{
    color:#0f0;
}
.conTitle p{
    font-size:20px;
    font-weight:600;
    color:#fff;
}
.mSend{
    width:80%;
    height:40%;
    margin:0 auto;
}
.mSend::after{
    content:"";
    display:block;
    clear:both;
}
.custInpo{
    float:left;
    width:70%;
    height:100%;
}
.custInpo ul{
    width:100%;
    height:100%;
}
.custInpo li{
    width:100%;
    height:10%;
    border:2px solid #888;
    border-bottom:none;
    background:#333;
}
.custInpo li:nth-child(3){
    height:40%;
    border-bottom:2px solid #888;
}
.custInpo li:last-child{
    border:none;
    background:none;
}
.custInpo div{
    width:150px;
    height:50px;
    margin:10px auto;
    text-align:center;
    background:#333;
    border:2px solid #888;
    cursor:pointer;
}
.custInpo span{
    font-size:20px;
    line-height:45px;
    color:#888;
}
.custInpo input{
    width:90%;
    height:100%;
    margin-left:10px;
    color:#fff;
    background:none;
    border:none;
}
.custInpo textarea{
    width:100%;
    height:100%;
    padding:10px;
    color:#fff;
    background:none;
}
.comInpo{
    float:right;
    width:30%;
    height:100%;
    padding:20px;
}
.comInpo h3{
    font-size:30px;
    font-weight:600;
    color:#fff;
    white-space:nowrap;
}
.comInpo address{
    font-size:16px;
    font-weight:600;
    line-height:50px;
    color:#fff;
    white-space:nowrap;
}
.oPage{
    width:80%;
    height:30%;
    margin:0 auto;
}
.oPage ul{
    width:100%;
    height:100%;
}
.oPage ul::after{
    content:"";
    display:block;
    clear:both;
}
.oPage li{
    float:left;
    width:25%;
    height:100%;
}
.oPage li a{
    display:inline-block;
    width:100%;
    height:100%;
    margin:0 auto;
}
.oPage img{
    width:40%;
    margin:25%;
}
.fancyBg{
    position:fixed;
    width:0;
    height:0;
    top:50%;
    left:50%;
    background:rgba(0,0,0,0.5);
    z-index:-1;
}
.fancyBx{
    overflow:hidden;
    position:absolute;
    max-width:80%;
    max-height:80%;
    top:50%;
    left:50%;
    padding:10px;
    background:#fff;
    transform:translate(-50%,-50%);
}
.fancyImg img{
    width:100%;
    height:100%;
}
@media (max-width:1024px){
    /* title cover text */
    .aground strong{
        font-size:100px;
    }
    /* tnb */
    .tnbWrap{
        height:120px;
    }
    .tnb{
        width:100%;
    }
    .tnb.fix{
        height:120px;
    }
    .tnb h1{
        width:100%;
        text-align:center;
    }
    .tnb li{
        width:25%;
    }
    .tnbMenu strong{
        font-size:16px;
        font-weight:800;
        color:#fff;
    }
    /* portfolio */
    .poInner{
        height:68%;
    }
    .poMenu{
        float:unset;
        width:100%;
        height:15%;
    }
    .poMenu::after{
        content:"";
        display:block;
        clear:both;
    }
    .poMenu ul{
        height:100%;
        margin:0;
    }
    .poMenu li{
        float:left;
        width:20%;
        height:100%;
    }
    .poList{
        width:100%;
        height:72%;
    }
    /* profile */
    .proImg{
        display:none;
    }
    .proText{
        width:100%;
    }
    /* contact */
    .mSend{
        height:60%;
    }
    .custInpo{
        float:unset;
        width:100%;
        height:50%;
    }
    .comInpo{
        float:unset;
        width:100%;
        height:50%;
        text-align:center;
    }
    .comInpo h3{
        font-size:24px;
        font-weight:600;
        color:#fff;
        white-space:nowrap;
    }
    .comInpo address{
        font-size:12px;
        line-height:30px;
    }
    .oPage{
        height:20%;
    }
}