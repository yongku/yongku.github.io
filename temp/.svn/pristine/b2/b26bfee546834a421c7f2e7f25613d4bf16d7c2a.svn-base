</div>

<script type="text/javascript">

	// 보기편하게 여러줄로 코딩
	$( '.css_test img' )
	.draggable({
		stop : function(){ // 드래그 종료시 실행
			$(this).animate({ top : 0, left : 0 }, 1000, 'easeOutElastic' ); // 원위치로
			// 제이쿼리UI 의 이징효과 사용
		}
	})

</script>

