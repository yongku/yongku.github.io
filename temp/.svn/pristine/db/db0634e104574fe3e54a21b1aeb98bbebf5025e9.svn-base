/* body Layout */
body{
    background-color:#222;
}
/* sub Layout */
.wrap{
    background-color:#fff;
}
/* header Layout */
.headerwrap{
    position:fixed;
    top:0;
    left:0;
    width:100%;
    height:90px;
    background:rgba(0,0,0,.6);
}
.header{
    position:relative;
    width:1100px;
    margin:0 auto;
}
/* .header content design */
.header h1{
    position:absolute;
    top:32px;
    left:20px;
    width:94px;
    height:29px;
}
.header h1 img{
    width:94px;
}
.header .tnb{
    position:absolute;
    top:49px;
    left:151px;
    line-height:0.5;
    text-align:center;
}
.header .tnb li{
    display:inline-block;
    margin-right:10px;
    line-height:0.5;
    font-size:13px;
    color:#fff;
}
.header .tnb a{
    line-height:0.5;
    color:inherit;
}
/* header nav design */
.header .nav{
    float:right;
}
/* nav - depth1 */
.header .dep1{
    position:relative;
    display:inline-block;
    height:90px;
    padding:0 20px 0 18px;
    line-height:90px;
    font-size:16px;
    font-weight:600;
    color:#fff;
}
.header .dep1 > a{
    position:relative;
    line-height:inherit;
    font-size:inherit;
    font-weight:inherit;
    color:inherit;
}
.header .dep1 > a::after{
    content:"";
    position:absolute;
    display:inline-block;
    width:0;
    height:2px;
    left:50%;
    bottom:-9px;
    background-color:#b9d0ff;
    transition:all 0.5s;
}
.header .dep1:hover > a{
    color:#b9d0ff;
}
.header .dep1:hover > a::after{
    width:100%;
    height:2px;
    left:0;
    bottom:-9px;
}
/* nav - depth2 */
.header .dep2{
    position:absolute;
    overflow:hidden;
    width:165px;
    left:0; /* */
    max-height:0px;
    padding-bottom:0px;
    padding-left:18px;
    background-color:rgba(0,0,0,.6);
    transition:all .5s;
    opacity:0;
}
.header .dep1:hover .dep2{
    max-height:160px;
    padding-bottom:15px;
    opacity:1;
}
.header .dep2 > li{
    margin-bottom:12px;
    font-size:14px;
    color:#fff;
}
.header .dep2 > li > a{
    font-size:inherit;
    color:inherit;
}
/* visual Layout */
.visual{
    width:100%;
    height:690px;
    background:url(../images/test1/main.jpg) no-repeat 50% 0;
    color:transparent; /* 글씨 투명하게하기 */
    text-indent:-9999em; /* 글씨 들여들끼로 내어쓰기 firefox에는 영역 표시됨 */
}
/* container Layout */
.container{
    width:1100px;
    height:500px;
    margin:0 auto;
}
.container::after{
    content:"";
    display:block;
    clear:both;
}
.container .cwrap{
    width:100%;
    height:100%;
}
.container ul .biglo{
    position:relative;
    float:left;
    width:275px;
    height:250px;
    padding:50px 35px;
    border:1px solid #ededed;
}
.container ul .biglo a{
    color:#666;
}
.container ul li .ico{
    position:absolute;
    width:275px;
    height:250px;
}
.container ul li .ico .icoimg{
    position:absolute;
    top:57px;
    left:85px;
}
.container ul .ann h3{
    margin-bottom:20px;
    font-size:14px;
    font-weight:800;
}
.container ul .ann li{
    overflow:hidden;
    height:25px;
    line-height:25px;
    white-space:nowrap;
    text-overflow:ellipsis;
}
.container ul .ann .more{
    position:absolute;
    top:50px;
    right:35px;
}
.container ul .gallery{
    position:relative;
    width:550px;
    padding-right:0;
}
.container ul .gallery h3{
    margin-bottom:20px;
    font-size:14px;
    font-weight:800;
}
.container .gallery li{
    float:left;
    width:95px;
    height:95px;
    margin-right:30px;
    border-radius:50%;
}
.container .gallery li a{
    position:relative;
    overflow:hidden;
    display:inline-block;
    width:100%;
    height:100%;
    border-radius:50%;
}
.container .gallery li a .ghover{
    position:absolute;
    display:none;
    top:0px;
    left:0;
    width:100%;
    height:100%;
    background:rgba(0,0,0,.6);
}
.container .gallery li a .ghover p{
    overflow:hidden;
    position:absolute;
    width:80px;
    height:30px;
    top:40px;
    left:8px;
    font-size:12px;
    color:#fff;
    white-space:nowrap;
    text-overflow:ellipsis;
}
.container .gallery li a:hover .ghover{
    display:block;
}
.container ul .gallery .more{
    position:absolute;
    top:50px;
    right:35px;
}
/* footer Layout */
.footerwrap{
    width:100%;
    height:355px;
    background-color:#222;
}
.footer{
    position:relative;
    width:1100px;
    top:44px;
    margin:0 auto;
    text-align:center;
}
.footer .info1{
    width:100%;
    height:195px;
    border-bottom:1px solid #999;
}
.footer .info1 h3{
    position:relative;
    margin-bottom:20px;
    font-size:16px;
    color:#fff;
}
.footer .info1 h3::after{
    content:"";
    position:absolute;
    display:inline-block;
    width:38px;
    height:3px;
    left:535px;
    bottom:-9px;
    background-color:#b9d0ff;
}
.footer p{
    font-size:18px;
    line-height:30px;
    color:#545454;
}
.footer ul{
    position:absolute;
    margin-top:25px;
    left:430px;
}
.footer ul li{
    display:inline-block;
}
.footer ul li::after{
    content:"⦿";
    padding-left:4px;
    color:#999;
}
.footer ul li:last-child::after{
    display:none;
}
.footer ul li a{
    color:#999;
}
.footer .address{
    width:100%;
    height:160px;
    padding-top:50px;
    padding-bottom:50px;
    text-align:left;
}
.footer .address h3{
    position:absolute;
    right:0;
    width:95px;
    height:29px;
    background:url(../images/test1/flogo.png) no-repeat 0 0 ;
}
.footer address{
    font-size:16px;
    color:#545454;
}