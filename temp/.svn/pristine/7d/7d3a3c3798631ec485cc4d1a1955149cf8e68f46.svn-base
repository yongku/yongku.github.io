body{
    min-width:50%;
}
/* tnb style */
.tnbwrap{
    width:100%;
    height:32px;
    background:#f0f0f0;
}
.tnb{
    position:relative;
    width:1000px;
    margin:0 auto;
}
.tnb a{
    font-size:11px;
    color:#000;
}
.tnb .lefttnb{
    position:absolute;
    width:200px;
}
.tnb ul .top1{
    height:32px;
    float:left;
    margin-left:10px;
    line-height:32px;
    text-align:center;
}
.tnb ul .top1:hover ul{
    display:block;
}
.tnb ul ul{
    position:absolute;
    display:none;
    width:100px;
    height:100px;
    padding-top:10px;
    padding-bottom:10px;
    top:32px;
    left:40px;
    background:#fff;
    box-shadow:2px 2px 10px #888;
    z-index:1;
}
.tnb ul .top2{
    height:33.3%;
}
.tnb .righttnb{
    position:absolute;
    right:20px;
}
.tnb .righttnb li{
    height:32px;
    float:left;
    margin-left:10px;
    line-height:32px;
    text-align:center;
}
/* 검색 및 메뉴 style */
.total{
    position:relative;
    width:1000px;
    margin:0 auto;
    transition:all 1s;
}
.total > .bigmanu ul a{
    font-size:12px;
    color:#000;
}
.mainmanu{
    position:absolute;
    width:110px;
    height:110px;
    background:url(../images/cupang/manu2.jpg) no-repeat 0 0;
}
.total .bigmanu > ul{
    position:absolute;
    display:none;
    width:800px;
    height:500px;
    top:110px;
    background:#fff;
    border:1px solid #888;
    box-shadow:2px 2px 10px #888;
    z-index:2;
}
.total > .bigmanu ul li{
    width:200px;
    height:30px;
    margin-left:20px;
}
.submanu{
    position:absolute;
    width:100%;
    height:100%;
    line-height:30px;
}
.manubar1 .menu1{
    position:absolute;
    display:none;
    bottom:0px;
    right:0px;
}
.manubar2{
    position:absolute;
    display:none;
    width:600px;
    height:500px;
    top:-1px;
    left:198px;
    border:1px solid #888;
    border-right:none;
}
.subback{
    position:absolute;
    bottom:0;
    right:0;
    width:225px;
    height:225px;
    background:url(../images/summer.jpg) no-repeat 0 0;
}
.manubar3{
    position:absolute;
    display:none;
    width:400px;
    height:500px;
    top:-1px;
    left:200px;
    border:1px solid #888;
}
.total > .bigmanu .manubar1 a:hover{
    color:#3b8df1;
    text-decoration:underline;
    font-weight:800;
    background:url(../images/cupang/bul_right.png) no-repeat 160px 10px;
    background-size:8px 8px;
}
.total > .bigmanu .manubar1 .manubar3 a:hover{
    background-size:0;
}
.total .bigmanu:hover > ul{
    display:block;
}
.total .bigmanu > ul > li:hover > ul{
    display:block;
}
.total .bigmanu > ul > li > ul > li:hover > ul{
    display:block;
}
.mback:hover > .menu1{
    display:block;
}
/* 검색창 */
.total .searchWrap{
    position:relative;
    width:880px;
    height:110px;
    padding-left:20px;
    left:110px;
}
.total .searchWrap .search{
    position:relative;
    width:100%;
    height:80px;
}
.total .searchWrap .search .tlogo{
    position:absolute;
    width:200px;
    top:20px;
}
/* 검색창 */
.total .searchWrap .search .sWindow{
    position:absolute;
    height:44px;
    top:25px;
    left:200px;
    border:2px solid #3b8df1;
}
.total .searchWrap .search .sWindow li{
    float:left;
}
.total .searchWrap .search .sWindow .glass{
    width:25px;
    height:25px;
}
.total .searchWrap .search .sWindow select{
    width:100px;
    height:40px;
    padding-left:10px;
    border:none;
}
.total .searchWrap .search .sWindow input{
    height:39.5px;
    border-top:none;
    border-bottom:none;
    border-right:none;
}
/* my manu */
.total .searchWrap .search .my{
    position:absolute;
    top:15px;
    right:90px;
}
.total .searchWrap .search .basck{
    position:absolute;
    top:4px;
    right:5px;
}
/* quick manu */
.total .searchWrap .quick li{
    float:left;
    position:relative;
    width:80px;
    height:30px;
}
.total .searchWrap .quick li a{
    position:absolute;
    width:100%;
    height:100%;
    text-align:center;
    line-height:30px;
}
/* visual */
.vWrap{
    width:100%;
}
/* visual ad */
.vWrap .swiper-container{
    width:100%;
    background:#eee;
}
.vWrap .swiper-wrapper{
    width:1900px;
    height:450px;
    margin:0 auto;
    background:#0ff;
}
.vWrap .swiper-pagination-bullet{
    width:180px;
    height:60px;
    border-radius:0;
    opacity:1;
}
.swiper-pagination-bullet-active{
    border:1px solid #3b8df1;
}
.vWrap .swiper-pagination-bullet:nth-child(1){
    background:url(../images/cupang/mainad1_1.jpg) no-repeat 0 0;
}
.vWrap .swiper-pagination-bullet:nth-child(2){
    background:url(../images/cupang/mainad2_1.jpg) no-repeat 0 0;
}
.vWrap .swiper-pagination-bullet:nth-child(3){
    background:url(../images/cupang/mainad3_1.jpg) no-repeat 0 0;
}
.vWrap .swiper-pagination-bullet:nth-child(4){
    background:url(../images/cupang/mainad4_1.jpg) no-repeat 0 0;
}
.vWrap .swiper-pagination-bullet:nth-child(5){
    background:url(../images/cupang/mainad5_1.jpg) no-repeat 0 0;
}
/* visual today */
/* visual today 제목 */
.vWrap .vToday{
    width:1000px;
    height:1060px;
    margin:0 auto;
}
.vWrap .vToday h2{
    height:80px;
    line-height:80px;
    color:#666;
}
.vWrap .vToday h2 span{
    line-height:80px;
    font-size:24px;
    font-weight:600;
    color:#000;
}
.vWrap .vToday h2 span::after{
    content:"ㅣ";
    font-size:20px;
}
/* visual today 내용 */
.vWrap .vToday > ul{
    width:100%;
    height:980px;
    border:1px solid #aaa;
}
.vWrap .vToday > ul > li{
    width:100%;
    height:300px;
}
.vWrap .vToday > ul > li > ul{
    width:100%;
    height:100%;
    margin-top:20px;
    margin-left:15px;
}
.vWrap .vToday > ul > li > ul > li{
    position:relative;
    float:left;
    width:230px;
    height:100%;
    margin:0 5px;
    border:1px solid #aaa;
}
.vWrap .vToday > ul > li > ul > li > .bBuy1{
    position:absolute;
    bottom:20px;
    left:20px;
    width:40px;
    height:40px;
    background:url(../images/cupang/buy1.jpg) no-repeat 0 0;
    z-index:1;
}
.vWrap .vToday > ul > li > ul > li > .bBuy2{
    position:absolute;
    display:none;
    bottom:20px;
    left:20px;
    width:97px;
    height:40px;
    background:url(../images/cupang/buy2.jpg) no-repeat 0 0;
    background-size:97px 40px;
    z-index:2;
}
.vWrap .vToday > ul > li > ul > li:hover{
    border:1px solid #1373e7;
}
.vWrap .vToday > ul > li > ul > li:hover > .bBuy2{
    display:block;
}
.vWrap .vToday > ul > li > ul > .wToday{
    width:470px;
}
/* visual Late*/
.vWrap .vLate{
    width:1000px;
    height:500px;
    margin:0 auto;
}
.vWrap .vLate h2{
    width:100%;
    height:80px;
    font-size:24px;
    font-weight:600;
    line-height:80px;
}
.vWrap .vLate > ul{
    position:relative;
    width:100%;
    height:420px;
}
.vWrap .vLate > ul > li{
    width:100%;
}
.vWrap .vLate > ul > li > a > div{
    position:absolute;
    width:250px;
    height:100px;
    padding:10px;
    border:1px solid #ccc;
    border-bottom:1px solid #1373e7;
}
.vWrap .vLate > ul > li.on > a > div{
    border:1px solid #1373e7;
    border-bottom:none;
}
.vWrap .vLate > ul > li:nth-child(2) > a > div{
    left:250px;
}
.vWrap .vLate > ul > li:nth-child(3) > a > div{
    left:500px;
}
.vWrap .vLate > ul > li:last-child > a > div{
    left:750px;
}
.vWrap .vLate > ul > li > a > div > img{
    float:left;
    border:1px solid #666;
    margin-right:10px;
}
.vWrap .vLate > ul > li > a > div strong{
    font-weight:600;
}
.vWrap .vLate > ul > li > a > div > em{
    color:#1373e7;
}
.vWrap .vLate > ul > li > a > div > em:hover{
    text-decoration:underline;
}
.vWrap .vLate > ul > li > .inItem{
    position:absolute;
    display:none;
    width:100%;
    height:320px;
    top:100px;
    border:1px solid #1373e7;
    border-top:none;
}
.vWrap .vLate > ul > li.on > .inItem{
    display:block;
}
.vWrap .vLate > ul > li > .inItem > h3{
    height:40px;
    margin-left:20px;
    font-size:14px;
    font-weight:600;
    line-height:50px;
}
.vWrap .vLate > ul > li > .inItem > ul{
    position:relative;
    display:block;
    width:100%;
    height:280px;
}
.vWrap .vLate > ul > li > .inItem > ul::after{
    content:"";
    display:block;
    clear:both;
}
.vWrap .vLate > ul > li > .inItem > ul > li{
    float:left;
    width:175px;
    height:230px;
    margin-left:20px;
}
.vWrap .vLate > ul > li > .inItem > ul > li div{
    overflow:hidden;
    width:100%;
    height:100%;
    text-overflow:ellipsis;
}
.vWrap .vLate > ul > li > .inItem > ul > li img{
    width:100%;
    height:150px;
    margin-bottom:20px;
    border:1px solid #999;
}
.vWrap .vLate > ul > li > .inItem > ul > li img:hover{
    border:1px solid #1373e7;
}
.vWrap .vLate > ul > li > .inItem > ul > li strong{
    margin:10px;
    line-height:30px;
    white-space:nowrap;
}
.vWrap .vLate > ul > li > .inItem > ul > li b{
    margin:10px;
    font-size:14px;
    font-weight:600;
}
